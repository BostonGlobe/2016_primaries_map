path
	fill none

.parties
	border solid red 1px
	width 100%
	overflow hidden

	.party
		width 50%
		float left

svg.choropleth

	display block
	margin auto
	vertical-align bottom

	g path
		stroke-linecap round
		stroke-linejoin round

	g.outline
		fill none
		stroke black
		stroke-width 0.2px

	g.features path
		stroke-width 0.1px
		stroke-opacity 0.1

		&.blank
			stroke black
			stroke-opacity 1
			fill white
